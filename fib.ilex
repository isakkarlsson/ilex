# Calculate the nth Fibonacci number
#
begin
  u := 0
  v := 1
  t := 0
  i := 2
  write "Enter a number: "
  read nth

  if nth > 1
   begin
     write "Input number is greater than 1\n"
   end
  else
   begin
     write "Input number is less than\n"
   end
  
  
  while i <= nth
   begin
     t := u + v
     u := v
     v := t
     i := i + 1
   end
  write "The "
  write nth
  write " fibonacci number is "
  write v  
end
